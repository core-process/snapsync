{
  "name": "snapsync",
  "version": "1.0.10",
  "description": "An excellent library and toolset for optimized transfers of single files and directory snapshots based on librsync.",
  "main": "./index.js",
  "author": "Niklas Salmoukas <niklas@salmoukas.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/core-process/snapsync.git"
  },
  "binary": {
    "module_name": "snapsync",
    "module_path": "./lib/binding/{configuration}/{node_abi}-{platform}-{arch}-{libc}/",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}-{libc}.tar.gz",
    "host": "https://github.com/core-process/snapsync/releases/download/",
    "remote_path": "{version}"
  },
  "dependencies": {
    "nan": "^2.7.0",
    "node-pre-gyp": "^0.6.39",
    "node-pre-gyp-github": "^1.3.1"
  },
  "scripts": {
    "preinstall": "npm install node-pre-gyp",
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "node index.js",
    "dev-configure": "node-pre-gyp configure",
    "dev-build": "node-pre-gyp build",
    "dev-package": "node-pre-gyp package",
    "dev-publish": "node-pre-gyp-github publish --release",
    "dev-all": "npm run dev-configure && npm run dev-build && npm run dev-package && npm run dev-publish"
  }
}
